  "version": "1.0.0",
  "description": "{{description}}",
  "main": "index.js",
  "scripts": {
    "build:dll": "NODE_ENV=development webpack --config ./config/webpack-config/dll.config.js",
    "build:dll:min": "NODE_ENV=production webpack --config ./config/webpack-config/dll.config.js -p",
    "build:dev": "webpack --config ./config/webpack.dev.js --watch",
    "build:prd": "node ./config/transferAsset-git.js",
    "build:doc": "apidoc -i src/service/ -o apidoc/",
    "check:modules": "node ./config/webpack-config/checkVersion.js",
    "isSkipPackage": "node ./config/webpack-config/checkBeforeCompile.js",
    "start": "DEBUG=true npm-run-all isSkipPackage  build:dll build:server",
    "dev": "npm-run-all isSkipPackage build:dll build:dev",
    "pub": "./script/pub.sh",
    "prd": "npm-run-all isSkipPackage build:dll:min build:prd",
    "debug": "DEBUG=true npm-run-all check:modules start",
    "test": "jest"
  },
  "keywords": [
    "开发工具",
    "webpack",
    "v5"
  ],
  "repository": {
